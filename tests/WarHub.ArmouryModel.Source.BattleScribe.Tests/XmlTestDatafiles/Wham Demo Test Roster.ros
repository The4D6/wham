            <selection id="4e9c-8ded-0bc7-6d24" name="MV36 Guardian Drone" entryId="b450-1a73-0648-b275::31b8-7770-558f-5e28::db14-5634-462f-ac89" entryGroupId="b450-1a73-0648-b275::31b8-7770-558f-5e28::22c8-bfa6-35c9-ef15" number="1" type="upgrade">

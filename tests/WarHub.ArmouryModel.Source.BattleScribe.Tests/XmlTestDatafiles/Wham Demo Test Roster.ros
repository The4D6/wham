                <profile id="aee5-92d1-5c04-8b30::a78a-402f-54f1-9db6::9994-6ceb-c8e6-49ac::583c-67d5-9de2-e763::c782-7940-e7aa-cef8" name="MV1 Gun Drone" hidden="false" profileTypeId="800f-21d0-4387-c943" profileTypeName="Unit">
